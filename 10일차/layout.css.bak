
html, body{height:100%;}

.con-area{
	width:1200px; 
	margin:0 auto;
}

.con-area::after{
	content:'';
	display:block;
	clear:both;
}

#wrap{
	position:relative;
	min-height:100%;
	background-image:url(img/background-img.jpg);
	background-repeat:no-repeat;
	background-size:cover;
	background-position:center;
}
#header{
	position:relative;
	background-color:blue;
	text-align:center;
}

#header #logo{
	display:inline-block;
}

#header #logo a{
	display:block;
	padding:20px 0;
	color:#fff;
	font-size:24px;
}

#main{
	position:relative;

}

#main #section-01{
	padding-top:50px;
	text-align:center;
}

#main #section-01 .info-wrap{
	display:inline-block;
}

#main #section-01 .info-wrap p{
	margin-bottom:10px;
	color:#fff;
	text-shadow:3px 3px 3px #000;
}

#main #section-01 .info-wrap h2{
	color:#fff;
	font-size:24px;
	text-shadow:3px 3px 3px #000;
}

#main #section-02{
	padding-top:50px;
}

#main #section-02 .l-con{
	float:left;
	width:50%;
	padding-right:15px;
}

#main #section-02 .l-con .con-box{
height:420px;
	padding:25px;
	border-radius:15px;
	background-color:beige;
}

#main #section-02 .l-con .con-box h3{
	color:#fff;
	text-align:center;
	margin-bottom:15px;
	font-size:20px;
	text-shadow:3px 3px 3px #000;
}
#main #section-02 .l-con .con-box .text-box{
	margin-bottom:15px;
	color:#000;
	font-size:17px;
}

#main #section-02 .l-con .l-con .con-box .btn-wrap{
	margin-top:15px;
}

#main #section-02 .l-con .con-box .btn-wrap::after{
	content:'';
	display:block;
	clear:both;
}

#main #section-02 .l-con .con-box .btn-wrap .btn{
	display:inline-block;
	padding:10px;
	border-radius:15px;
}

#main #section-02 .l-con .con-box .btn-wrap .btn.btn-01{
	float:left;
	color:#fff;
	background:blue;
}
#main #section-02 .l-con .con-box .btn-wrap .btn.btn-02{
	float:right;
	color:#fff;
	background:red;
}

#main #section-02 .r-con{
	float:right;
	width:50%;
	padding-left:15px;
}

#main #section-02 .r-con .quick-menu{
	
}

#main #section-02 .r-con .quick-menu::after{
	content:''; display:block; clear:both;
}
#main #section-02 .r-con .quick-menu > li{
	float:left;
	width:33.33%;
	padding:0 10px;
	margin-bottom:20px;
}

#main #section-02 .r-con .quick-menu > li.bm_0{
	margin-bottom:0;
}
#main #section-02 .r-con .quick-menu > li > a::hover{border-color:red;}
#main #section-02 .r-con .quick-menu > li > a {
	display:block;
	height:200px;
	padding-top:15px;
	text-align:center;
}

#main #section-02 .r-con .quick-menu > li > a:nth-child(1){
	background-color:#F0F8FF;
}

#main #section-02 .r-con .quick-menu > li > a:nth-child(2){
	background-color:#FAEBD7;
}

#main #section-02 .r-con .quick-menu > li > a:nth-child(3){
	background-color:#00FFFF;
}

#main #section-02 .r-con .quick-menu > li > a:nth-child(4){
	background-color:#7FFFD4;
}

#main #section-02 .r-con .quick-menu > li > a:nth-child(5){
	background-color:#F0FFFF ;
}
#main #section-02 .r-con .quick-menu > li > a:nth-child(6){
	background-color:#F0F8FF;
}
#main #section-02 .r-con .quick-menu > li > a > figure{
	text-align:center;
}
#main #section-02 .r-con .quick-menu > li > a > figure > img{
	width:65px;
	margin-bottom:10px;
}

#main #section-02 .r-con .quick-menu > li > a > strong{
	display:block;
	margin-bottom:10px;
}

#main #section-02 .r-con .quick-menu > li > a > strong{
	display:block;
	margin-bottom:10px;
}
#footer{
	padding:15px 0;
}

#footer .copyright{
	text-align:center;
	font-size:20px;
	color:#fff;
}






